    <script>
        parent.GetChildValue('login');
        function denglu(e){
            var phone = $('input[name="phone"]').val();//手机号
            var password = $('input[name="password"]').val();//密码
            var code = $('input[name="code"]').val();//验证码
            if(!phone){
                layer.msg('please fill account');return;
            }if(!password){
                layer.msg('Please fill password');return;
            }if(!code){
                layer.msg('please fill captcha');return;
            }
            $(e).attr('disabled','disabled');//提交ajax时禁用按钮
            $.ajax({
                type:"POST",
                url:"/Home/Login/logo",
                data:{
                    account:phone,
                    pwd:password,
                    code:code
                },
                //请求成功
                success : function(result) {
                    if(result.status==200){
                        location.href="/Home/Index/index";
                        return;
                    }else{
                        $('.codeimg').attr('src',"/Home/Login/verify/r/539")
                        layer.msg(result.msg);
                    }
                    $(e).removeAttr('disabled');//提交成功时启用按钮
                },
                //请求失败，包含具体的错误信息
                error : function(e){
                    console.log(e.status);
                    console.log(e.responseText);
                }
            });
        }
    </script>
